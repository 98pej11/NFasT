{
  "extends": ["airbnb", "plugin:prettier/recommended"],
  "env": {
    "browser": true
  },
  "rules": {
    "no-shadow": "off",
    "prettier/prettier": [
            "error",
            {
                "endOfLine": "auto"
            }
        ],
        "react/function-component-definition":[2, {"namedcomponents":"arrow-function"}]
  }
}
